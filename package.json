{
  "name": "see-eat-sleep",
  "version": "0.0.0",
  "description": "Example of building a larger app with multiple smaller apps using npm and browserify.",
  "main": "index.js",
  "scripts": {
    "preinstall": "sh ./preinstall.sh",
    "postinstall": "npm dedupe",
    "start": "nodemon -e js,css,hbs,json ./node_modules/.bin/appup --config ./appup-dev-config.js --api 4000 --pages 3000 ./client/init",
    "test": "(cd ./ses-core && npm test) && (cd ./ses-see && npm test) && (cd ./ses-eat && npm test)"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/thlorenz/see-eat-sleep.git"
  },
  "homepage": "https://github.com/thlorenz/see-eat-sleep",
  "dependencies": {},
