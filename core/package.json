{
  "name": "core",
  "version": "0.0.0",
  "description": "The core app which is used by all the sub apps.",
  "main": "./servers/index.js",
  "browser": "./client/core.js",
  "scripts": {
    "start": "nodemon -e js,css,hbs,json ./node_modules/.bin/appup --config ./appup-dev-config.js --api 4000 --pages 3000 ./client/init",
    "test": "zuul client/test/views/ --config ./zuul-test-config.js",
    "start-test": "zuul client/test/views/ --server 3002 --config ./zuul-test-config.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/thlorenz/core.git"
  },
  "homepage": "https://github.com/thlorenz/core",
  "dependencies": {
    "hyperwatch": "~0.5.0",
    "handlebars": "~1.0.12",
    "require-all": "0.0.6",
    "hyperquest": "~0.1.7",
    "bytes": "~0.2.1",
    "log-request": "0.0.1",
    "render-routes": "~0.1.1",
    "underscore": "~1.5.1",
    "backbone": "~1.0.0"
  },
  "devDependencies": {
    "nodemon": "~0.7.8",
    "zuul": "git://github.com/shtylman/zuul",
    "appup": "0.0.3"
  },
  "keywords": [],
  "author": {
    "name": "Thorsten Lorenz",
    "email": "thlorenz@gmx.de",
    "url": "http://thlorenz.com"
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/thlorenz/core/blob/master/LICENSE"
  },
  "engine": {
    "node": ">=0.6"
  }
}
