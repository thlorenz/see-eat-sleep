{
  "name": "ses-see",
  "version": "0.0.0",
  "description": "the portion of the app where you get to see things",
  "main": "./servers/index.js",
  "browser": "./client/see.js",
  "browserify": {
    "transform": [
      "hbsfy"
    ]
  },
  "scripts": {
    "preinstall": "sh ./preinstall.sh",
    "postinstall": "npm dedupe",
    "start": "nodemon -e js,css,hbs,json appup --config ./appup-dev-config.js --api 4000 --pages 3000 ./client/init",
    "test": "zuul client/test/views/ --config ./zuul-test-config.js",
    "start-test": "zuul client/test/views/ --server 3002 --config ./zuul-test-config.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/thlorenz/see.git"
  },
  "homepage": "https://github.com/thlorenz/see",
  "optionalDependencies": {},
  "dependencies": {
    "hbsfy": "~0.1.5",
    "backbone": "~1.0.0",
    "underscore": "~1.5.1",
    "express": "~3.3.4"
  },
  "devDependencies": {
    "nodemon": "~0.7.8",
    "mocha": "~1.12.0",
    "sinon": "~1.7.3",
    "zuul": "git://github.com/shtylman/zuul",
    "browserify": "~2.26.0",
    "optimist": "~0.6.0",
    "mold-source-map": "~0.2.0",
    "restify": "~2.6.0"
  },
  "keywords": [],
  "author": {
    "name": "Thorsten Lorenz",
    "email": "thlorenz@gmx.de",
    "url": "http://thlorenz.com"
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/thlorenz/see/blob/master/LICENSE"
  },
  "engine": {
    "node": ">=0.6"
  }
}
