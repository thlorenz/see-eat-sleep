{
  "name": "ses-see",
  "version": "0.0.0",
  "description": "the portion of the app where you get to see things",
  "main": "./server/index.js",
  "browser": "./client/see.js",
  "browserify": {
    "transform": [
      "hbsfy"
    ]
  },
  "scripts": {
    "preinstall": "sh ./preinstall.sh",
